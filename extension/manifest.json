{
  "name": "会議なう",
  "version": "1",
  "manifest_version": 2,
  "description": "会議のアジェンダ，残り時間，参加メンバーを表示します．ワンクリックで確認！",
  "icons": {
    "16" : "images/sample_16.png",
    "48" : "images/sample_48.png",
    "128" : "images/sample_128.png"
  },
  "browser_action": {
    "default_icon": "images/sample_16.png",
    "default_title": "Meeting just on time",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "css": ["css/contents_style.css"]
    // "page": "background.html"
    },
  "permissions":["notifications"],
  "content_security_policy": "img-src 'self' http://www.google.com/favicon.ico; object-src 'self'; script-src 'self'"
}
